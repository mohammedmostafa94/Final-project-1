Code#2:
def count_frequencies(words):
    frequency = {}
    
    for word in words:
        if word in frequency:
            frequency[word] += 1
        else:
            frequency[word] = 1

    return frequency

# Example usage
input_strings = ["apple", "banana", "apple", "orange", "banana", "apple"]

frequency_count = count_frequencies(input_strings)

print("Frequency Count:", frequency_count)
